<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="429dp"
        android:layout_height="84dp"
        android:background="@color/colorAccent"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/restaurant_1"
        android:layout_width="286dp"
        android:layout_height="96dp"
        android:layout_marginTop="16dp"
        android:text="Restaurant Name"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/restaurant_"
        android:layout_width="286dp"
        android:layout_height="96dp"
        android:layout_marginTop="8dp"
        android:text="Restaurant Name"
        app:layout_constraintTop_toBottomOf="@+id/restaurant_1"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/restaurant_2"
        android:layout_width="286dp"
        android:layout_height="96dp"
        android:layout_marginTop="8dp"
        android:text="Restaurant Name"
        app:layout_constraintTop_toBottomOf="@+id/restaurant_"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/restaurant_3"
        android:layout_width="286dp"
        android:layout_height="96dp"
        android:layout_marginTop="8dp"
        android:text="Restaurant Name"
        app:layout_constraintTop_toBottomOf="@+id/restaurant_2"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/restaurant_4"
        android:layout_width="286dp"
        android:layout_height="96dp"
        android:layout_marginTop="8dp"
        android:text="Restaurant Name"
        app:layout_constraintTop_toBottomOf="@+id/restaurant_3"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/restaurant_5"
        android:layout_width="286dp"
        android:layout_height="96dp"
        android:layout_marginTop="8dp"
        android:text="Restaurant Name"
        app:layout_constraintTop_toBottomOf="@+id/restaurant_4"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="111dp"
        android:layout_height="96dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        android:text="Food Type"
        app:layout_constraintStart_toEndOf="@+id/restaurant_1"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="111dp"
        android:layout_height="96dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Food Type"
        app:layout_constraintStart_toEndOf="@+id/restaurant_1"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="111dp"
        android:layout_height="96dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Food Type"
        app:layout_constraintStart_toEndOf="@+id/restaurant_1"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="111dp"
        android:layout_height="96dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Food Type"
        app:layout_constraintStart_toEndOf="@+id/restaurant_1"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="111dp"
        android:layout_height="96dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Food Type"
        app:layout_constraintStart_toEndOf="@+id/restaurant_1"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="111dp"
        android:layout_height="96dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Food Type"
        app:layout_constraintStart_toEndOf="@+id/restaurant_1"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

</android.support.constraint.ConstraintLayout>